{
    "name": "GoogleTranslate Hotkey",
    "version": "1.3",
    "description": "",
    "permissions": [
    ],
	"icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
		{
			"matches": ["http://translate.google.com/*"],
			"js": [
				"translate_hotkey.js"
			]
		},
		{
			"matches": ["http://www.google.com/dictionary*"],
			"js": [
				"dictionary_hotkey.js"
			]
		}
	]
}